{
  "name": "ocaml-indent",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "Auto-indent ocaml",
  "keywords": [
    "ocaml",
    "indent",
    "ocp-indent"
  ],
  "activationHooks": [
    "language-ocaml:grammar-used"
  ],
  "repository": "https://github.com/314eter/atom-ocaml-indent",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "ocamlIndent": {
      "versions": {
        "0.3.0": "provideIndent"
      }
    }
  },
  "configSchema": {
    "ocp-indentPath": {
      "type": "string",
      "default": "ocp-indent"
    }
  }
}
